<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/"
            factory-class="se.redfield.bert.nodes.tokenizer.BertTokenizerNodeFactory">
      </node>
      <node
            category-path="/"
            factory-class="se.redfield.bert.nodes.selector.BertModelSelectorNodeFactory">
      </node>
      <node
            category-path="/"
            factory-class="se.redfield.bert.nodes.embedder.BertEmbedderNodeFactory"
            id="se.redfield.bert.nodes.embedder.BertEmbedderNodeFactory">
      </node>
      <node
            category-path="/"
            factory-class="se.redfield.bert.nodes.classifier.BertClassifierNodeFactory">
      </node>
      <node
            category-path="/"
            factory-class="se.redfield.bert.nodes.predictor.BertPredictorNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#ff00ff"
            hidden="false"
            name="se.redfield.bert.nodes.port.BertModelPortObject"
            objectClass="se.redfield.bert.nodes.port.BertModelPortObject"
            objectSerializer="se.redfield.bert.nodes.port.BertModelPortObject$Serializer"
            specClass="se.redfield.bert.nodes.port.BertModelPortObjectSpec"
            specSerializer="se.redfield.bert.nodes.port.BertModelPortObjectSpec$Serializer">
      </portType>
      <portType
            name="se.redfield.bert.nodes.port.BertClassifierPortObject"
            objectClass="se.redfield.bert.nodes.port.BertClassifierPortObject"
            objectSerializer="se.redfield.bert.nodes.port.BertClassifierPortObject$Serializer"
            specClass="se.redfield.bert.nodes.port.BertClassifierPortObjectSpec"
            specSerializer="se.redfield.bert.nodes.port.BertClassifierPortObjectSpec$Serializer">
      </portType>
   </extension>
      <extension
         point="org.knime.python.modules">
      <directory
            path="py/">
      </directory>
   </extension>
   
</plugin>
